set nocompatible

execute pathogen#infect()

filetype plugin indent on
syn on se title
set incsearch
set number

" Kernel development
"set tabstop=8
"set softtabstop=8
"set shiftwidth=8
"set noexpandtab

" Chromium style
set tabstop=2
set softtabstop=2
set shiftwidth=2
set expandtab
set colorcolumn=80
